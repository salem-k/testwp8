.window.error {
	border: 1px solid #F00;
}

.window {
	background-color: #090909;
	padding: 1em;
	border-radius: 0.4em;
	border: 1px solid rgb(0, 129, 9);
	text-align: center;
}

.window.error button {
	background-color: #F00;
